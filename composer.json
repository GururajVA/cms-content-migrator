{
  "name": "name/cms-content-migrator",
  "description": "A tool to migrate content between Joomla and WordPress",
  "type": "project",
  "require": {
      "php": "^8.1",
      "symfony/http-client": "^6.3",
      "vlucas/phpdotenv": "^5.5",
      "monolog/monolog": "^3.4"
  },
  "require-dev": {
      "phpunit/phpunit": "^10.2"
  },
  "autoload": {
      "psr-4": {
          "CMSContentMigrator\\": "src/"
      }
  },
  "config": {
      "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}